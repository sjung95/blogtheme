            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;3 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt;  &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Questo codice è modificabile ed eseguibile!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;4 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt;  &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;このコードは編集して実行出来ます！&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
