            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;6 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt;  &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Ten kod można edytować oraz uruchomić!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;7 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt;  &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Este código é editável e executável!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;8 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt;  &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Этот код можно отредактировать и запустить!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;9 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt;  &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Bạn có thể edit và run code trực tiếp!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;10 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt; &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;这段代码是可以编辑并且能够运行的！&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;11 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt; &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Dieser Code kann bearbeitet und ausgeführt werden!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;12 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt; &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Den här koden kan redigeras och köras!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;13 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt; &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;Tento kód můžete upravit a spustit&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
            &lt;&#x2F;span&gt;&lt;span style=&quot;color:#a5c261;&quot;&gt;14 &lt;&#x2F;span&gt;&lt;span style=&quot;color:#cc7833;&quot;&gt;=&amp;gt; &lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;println!(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#c1be91;&quot;&gt;&amp;quot;این کد قابلیت ویرایش و اجرا دارد!&amp;quot;&lt;&#x2F;span&gt;&lt;span style=&quot;color:#e6e1dc;&quot;&gt;),
